{
  "name": "aframe-udemy",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "aframe": "^0.9.2"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.3",
    "standard": "^12.0.1"
  },
  "scripts": {
    "dev": "yarn rm:dev && parcel index.html",
    "build": "yarn rm:build && parcel build src/*.html -d build/ --no-content-hash --no-source-maps",
    "rm:dev": "rm -rf dist/ && yarn rm:cache",
    "rm:build": "rm -rf build/ && yarn rm:cache",
    "rm:cache": "rm -rf .cache"
  }
}
